{
  "name": "flecs-lang",
  "displayName": "Flecs",
  "description": "Syntax highlighting for flecs",
  "publisher": "flecs-hub",
  "author": {
    "name": "Sander Mertens"
  },
  "version": "1.0.3",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "flecs",
      "aliases": ["Flecs", "flecs"],
      "extensions": [".flecs"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "flecs",
      "scopeName": "source.flecs",
      "path": "./syntaxes/flecs.tmLanguage.json"
    }]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/flecs-hub/flecs-lang.git"
  }
}
